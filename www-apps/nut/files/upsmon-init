# $Header: /usr/local/ssd/gentoo-x86/output/www-apps/cvs-repo/gentoo-x86/www-apps/nut/files/Attic/upsmon-init,v 1.1 2004/08/15 15:19:10 stuart Exp $

depend() {
	need net
}

start() {
	ebegin "Starting upsmon"
	start-stop-daemon --start --quiet --exec /usr/sbin/upsmon
	eend $?
}

stop() {
	ebegin "Stopping upsmon"
	start-stop-daemon --stop --quiet --pidfile=/var/run/upsmon.pid
	eend $?
}
