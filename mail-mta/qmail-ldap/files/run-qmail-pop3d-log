#!/bin/bash
# Gentoo Startup script for qmail's POP3 daemon (logging)
# $Header: /usr/local/ssd/gentoo-x86/output/mail-mta/cvs-repo/gentoo-x86/mail-mta/qmail-ldap/files/Attic/run-qmail-pop3d-log,v 1.1 2007/06/16 13:48:10 hollow Exp $

SERVICE=pop3
. /var/qmail/bin/qmail-config-system && \
eval exec /usr/bin/setuidgid qmaill /usr/bin/multilog ${LOG_OPTS} ${LOG_DEST}
