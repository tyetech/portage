#!/bin/sh
# gentoo qmail rc file to start qmail-start and thus qmail-send
# $Header: /usr/local/ssd/gentoo-x86/output/mail-mta/cvs-repo/gentoo-x86/mail-mta/qmail/files/Attic/rc,v 1.2 2004/07/18 03:29:51 dragonheart Exp $

exec env - PATH="/var/qmail/bin:$PATH" \
	qmail-start "`grep -v '^#' /var/qmail/control/defaultdelivery`"
