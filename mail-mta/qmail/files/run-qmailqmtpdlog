#!/bin/sh
# Gentoo Startup script for qmail's QMTP daemon (logging)
# $Header: /usr/local/ssd/gentoo-x86/output/mail-mta/cvs-repo/gentoo-x86/mail-mta/qmail/files/Attic/run-qmailqmtpdlog,v 1.1 2004/05/30 10:50:13 robbat2 Exp $
SERVICE=qmtp
source /var/qmail/bin/qmail-config-system && \
exec /usr/bin/setuidgid qmaill /usr/bin/multilog ${LOG_OPTS} ${LOG_DEST}
