#!/bin/sh
# Gentoo Startup script for qmail's SMTP daemon (logging)
# $Header: /usr/local/ssd/gentoo-x86/output/mail-mta/cvs-repo/gentoo-x86/mail-mta/qmail/files/Attic/run-qmailsmtpdlog,v 1.2 2004/07/18 03:29:51 dragonheart Exp $
SERVICE=smtp
source /var/qmail/bin/qmail-config-system && \
exec /usr/bin/setuidgid qmaill /usr/bin/multilog ${LOG_OPTS} ${LOG_DEST}
