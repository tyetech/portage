#!/bin/bash
# Gentoo Startup script for qmail's SMTP daemon (logging)
# $Header: /usr/local/ssd/gentoo-x86/output/mail-mta/cvs-repo/gentoo-x86/mail-mta/qmail/files/Attic/run-qmailsmtpdlog,v 1.4 2005/01/04 21:35:13 hansmi Exp $
SERVICE=smtp
source /var/qmail/bin/qmail-config-system && \
eval exec /usr/bin/setuidgid qmaill /usr/bin/multilog ${LOG_OPTS} ${LOG_DEST}
