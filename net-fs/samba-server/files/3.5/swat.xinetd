# default: off
# description: SWAT is the Samba Web Admin Tool. Use swat \
#	       to configure your Samba server. To use SWAT, \
#	       connect to port 901 with your favorite web browser.
# $Header: /usr/local/ssd/gentoo-x86/output/net-fs/cvs-repo/gentoo-x86/net-fs/samba-server/files/3.5/Attic/swat.xinetd,v 1.1 2010/01/26 19:04:00 patrick Exp $

service swat
{
	port            = 901
	socket_type     = stream
	wait            = no
	only_from       = localhost
	user            = root
	server          = /usr/sbin/swat
	log_on_failure += USERID
	disable         = yes
}
