. /etc/pcmcia/shared
. /etc/conf.d/isdn

# Syslog output
syslog() {  # <msg>
	/usr/bin/logger -t "isdn-pcmcia" "$@"
}

case "${ACTION}" in
	start)
		# create ISDN devices if necessary
		[ -c /dev/isdninfo -a -c /dev/isdnctrl0 ] || /etc/init.d/isdn makedev
		;;
	stop)
		;;
esac
exit 0
