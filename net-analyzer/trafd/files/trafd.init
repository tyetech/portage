depend() {
	need net
	use cupsd
}

start() {
	ebegin "Starting trafd"
	/usr/bin/trafd -p -r -i ${IFACE}
	result=$?
	eend $result
}

stop() {
	ebegin "Stopping trafd"
	/usr/bin/trafstop ${IFACE} > /dev/null
	result=$?
	eend $result
}
