depend() {
	need logger
}

start() {
	ebegin "Starting inputd"
		start-stop-daemon --start --quiet --background --exec /usr/sbin/inputd
	eend $?
}

stop() {
	ebegin "Stopping inputd"
		start-stop-daemon --stop --quiet --name inputd
	eend $?
}
