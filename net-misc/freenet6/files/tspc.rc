depend() {
	need net
}

start() {
	ebegin "Starting Freenet6 IPv6 Client"
	tspc -f /etc/freenet6/tspc.conf
	eend $?
}

stop() {
	ebegin "Stopping Freenet6 IPv6 Client"
	kill `pidof tspc`
	eend $?
}
