#!/bin/sh
# Gentoo Startup script for qmail's SMTP daemon (logging)
# $Header: /usr/local/ssd/gentoo-x86/output/net-mail/cvs-repo/gentoo-x86/net-mail/qmail/files/Attic/run-qmailsmtpdlog,v 1.1 2003/11/30 11:32:06 robbat2 Exp $
SERVICE=smtp
source /var/qmail/bin/qmail-config-system && \
exec /usr/bin/setuidgid qmaill /usr/bin/multilog ${LOG_OPTS} ${LOG_DEST}
